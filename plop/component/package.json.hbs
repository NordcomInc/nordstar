{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-{{componentName}}",
    "type": "module",
    "version": "0.0.0",
    "description": "{{escapeJsonString description}}",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": [
                "./dist/index.js"
            ],
            "module": [
                "./dist/index.js"
            ],
            "types": [
                "./dist/index.d.ts"
            ]
        },
        "./*": {
            "import": [
                "./dist/*.js"
            ],
            "module": [
                "./dist/*.js"
            ],
            "types": [
                "./dist/*.d.ts"
            ]
        },
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite build --watch",
        "clean": "rm -rf dist coverage storybook-static .turbo .next *.tsbuildinfo *.log",
        "typecheck": "tsc --noEmit"
        "lint": "concurrently -i pnpm:lint:*",
        "lint:prettier": "prettier --check src/**/*.*",
        "format": "concurrently -i pnpm:format:*",
        "format:prettier": "prettier --write \"./**/*.{cjs,mjs,ts,tsx,json}\""
    },
    "keywords": [
        "nordstar",
        "nordcom",
        "component",
        "react",
        "{{componentName}}",
        "nordstar-{{componentName}}"
    ],
    "author": {
        "name": "Nordcom AB",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordstar.git",
        "directory": "packages/{{outDir}}/{{componentName}}"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordstar/issues"
    },
    "homepage": "https://nordstar.dev/docs/components/{{componentName}}/",
    "dependencies": {
        "@nordcom/nordstar-system": "workspace:*"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@types/node": "20.16.2",
        "@types/react-dom": "18.3.0",
        "@types/react": "18.3.8",
        "autoprefixer": "10.4.20",
        "postcss": "8.4.47",
        "prettier": "3.6.2",
        "react-dom": "19.1.0",
        "react": "19.1.0",
        "tailwindcss": "3.4.12",
        "typescript": "5.6.2",
        "vite": "5.4.7"
    }
}
