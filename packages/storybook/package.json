{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-storybook",
    "version": "0.0.48",
    "engines": {
        "node": "<=21"
    },
    "main": "src/index.ts",
    "prettier": "@nordcom/prettier",
    "sideEffects": true,
    "private": true,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "storybook build --disable-telemetry",
        "dev": "storybook dev -p 3001 --disable-telemetry",
        "clean": "rm -rf dist coverage storybook-static .turbo .next *.tsbuildinfo *.log"
    },
    "dependencies": {
        "@nordcom/nordstar": "workspace:*",
        "react-dom": "18.2.0",
        "react": "18.2.0",
        "storybook": "8.0.5"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@storybook/addon-essentials": "8.0.5",
        "@storybook/cli": "8.0.5",
        "@storybook/react-vite": "8.0.5",
        "@storybook/react": "8.0.5",
        "@types/node": "20.12.3",
        "@types/react-dom": "18.2.23",
        "@types/react": "18.2.74",
        "@vitejs/plugin-react": "4.2.1",
        "prettier": "3.2.5",
        "storybook-dark-mode": "4.0.1",
        "typescript": "5.4.3",
        "vite": "5.2.7"
    }
}
