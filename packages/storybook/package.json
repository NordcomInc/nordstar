{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-storybook",
    "version": "0.0.73",
    "main": "src/index.ts",
    "sideEffects": true,
    "private": true,
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "storybook build --disable-telemetry",
        "dev": "storybook dev -p 3001 --disable-telemetry",
        "clean": "rm -rf dist coverage storybook-static .turbo .next *.tsbuildinfo *.log"
    },
    "dependencies": {
        "@nordcom/nordstar": "workspace:*",
        "react-dom": "19.1.0",
        "react": "19.1.0",
        "storybook": "8.4.0"
    },
    "devDependencies": {
        "@joshwooding/vite-plugin-react-docgen-typescript": "0.4.1",
        "@nordcom/prettier": "0.1.1",
        "@storybook/addon-controls": "8.4.0",
        "@storybook/addon-docs": "8.4.0",
        "@storybook/addon-essentials": "8.4.0",
        "@storybook/blocks": "8.4.0",
        "@storybook/core": "8.4.0",
        "@storybook/manager-api": "8.4.0",
        "@storybook/react-vite": "8.4.0",
        "@storybook/react": "8.4.0",
        "@storybook/theming": "8.4.0",
        "@types/node": "24.0.10",
        "@types/react-dom": "19.1.6",
        "@types/react": "19.1.8",
        "@vitejs/plugin-react": "4.3.3",
        "autoprefixer": "10.4.20",
        "postcss-import": "16.1.0",
        "postcss": "8.5.6",
        "prettier": "3.6.2",
        "storybook-addon-react-docgen": "1.2.44",
        "storybook-addon-sass-postcss": "0.3.2",
        "storybook-dark-mode": "4.0.2",
        "tailwindcss": "3.4.14",
        "typescript": "5.8.3",
        "vite-plugin-dts": "4.5.4",
        "vite-tsconfig-paths": "5.1.4",
        "vite": "7.0.0"
    }
}
