{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-storybook",
    "type": "module",
    "engines": {
        "node": ">=18 <=21"
    },
    "main": "src/index.ts",
    "prettier": "@nordcom/prettier",
    "sideEffects": true,
    "private": true,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "storybook build",
        "dev": "storybook dev -p 3001",
        "clean": "rimraf -g dist coverage storybook-static .turbo .next .tsbuildinfo *.log"
    },
    "dependencies": {
        "@nordcom/nordstar": "workspace:*",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@storybook/addon-essentials": "8.0.0",
        "@storybook/cli": "8.0.0",
        "@storybook/react-vite": "8.0.0",
        "@vitejs/plugin-react": "4.2.1",
        "prettier": "3.2.5",
        "rimraf": "5.0.5",
        "storybook-dark-mode": "4.0.0",
        "typescript": "5.4.2",
        "vite": "5.1.6"
    }
}
