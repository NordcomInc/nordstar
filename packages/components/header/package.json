{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-header",
    "type": "module",
    "version": "0.0.73",
    "description": "Generic simple component to render headers for landing pages and similar.",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": [
                "./dist/index.js"
            ],
            "module": [
                "./dist/index.js"
            ],
            "types": [
                "./dist/index.d.ts"
            ]
        },
        "./*": {
            "import": [
                "./dist/*.js"
            ],
            "module": [
                "./dist/*.js"
            ],
            "types": [
                "./dist/*.d.ts"
            ]
        },
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite build --watch",
        "clean": "rm -rf dist coverage storybook-static .turbo .next *.tsbuildinfo *.log",
        "typecheck": "tsc --noEmit",
        "lint": "concurrently -i pnpm:lint:*",
        "lint:prettier": "prettier --check \"./**/src/**/*.{cjs,mjs,ts,tsx}\"",
        "format": "concurrently -i pnpm:format:*",
        "format:prettier": "prettier --write \"./**/src/**/*.{cjs,mjs,ts,tsx}\""
    },
    "keywords": [
        "nordstar",
        "nordcom",
        "component",
        "react",
        "header",
        "nordstar-header"
    ],
    "author": {
        "name": "Nordcom AB",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordstar.git",
        "directory": "packages/components/header"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordstar/issues"
    },
    "homepage": "https://nordstar.dev/docs/components/header/",
    "dependencies": {
        "@nordcom/nordstar-card": "workspace:*",
        "@nordcom/nordstar-system": "workspace:*",
        "@nordcom/nordstar-view": "workspace:*"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@nordcom/nordstar-label": "workspace:*",
        "@types/node": "24.0.10",
        "@types/react-dom": "19.1.6",
        "@types/react": "19.1.8",
        "autoprefixer": "10.4.20",
        "postcss": "8.5.6",
        "prettier": "3.6.2",
        "react-dom": "19.1.0",
        "react": "19.1.0",
        "tailwindcss": "3.4.14",
        "typescript": "5.8.3",
        "vite-plugin-dts": "4.5.4",
        "vite-tsconfig-paths": "5.1.4",
        "vite": "7.0.0"
    }
}
