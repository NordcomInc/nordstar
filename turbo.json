{
    "$schema": "https://turbo.build/schema.json",
    "baseBranch": "origin/master",
    "globalDependencies": ["tsconfig.json"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "dist/**", "lib/**"]
        },
        "typecheck": {
            "cache": false,
            "dependsOn": ["^typecheck"]
        },
        "start": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": []
        },
        "clean": {
            "cache": false,
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}
