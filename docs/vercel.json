{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "trailingSlash": true,
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/storybook/",
            "destination": "https://nordstar-storybook.vercel.app/"
        },
        {
            "source": "/storybook/:path*",
            "destination": "https://nordstar-storybook.vercel.app/:path*"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
