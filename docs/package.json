{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/nordstar-docs",
    "sideEffects": false,
    "private": true,
    "type": "module",
    "version": "0.0.73",
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "build": "next build",
