{
    "name": "nordstar",
    "version": "0.0.1",
    "description": "The @nordcom component library.",
    "main": "index.js",
    "prettier": "@nordcom/prettier",
    "workspaces": [
        "packages/core/*",
        "packages/components/*",
        "docs"
    ],
    "engines": {
        "npm": ">=8",
        "node": ">=18 <=21"
    },
    "scripts": {
        "prepare": "husky install",
        "dev": "concurrently -i --raw npm:dev:*",
        "dev:docs": "turbo dev --filter=@nordcom/nordstar-docs",
        "dev:packages": "",
        "build": "turbo build",
        "build:packages": "turbo build --filter=!@nordcom/nordstar-docs",
        "build:docs": "turbo build --filter=@nordcom/nordstar-docs",
        "typecheck": "turbo typecheck",
        "lint": "concurrently --raw npm:lint:*",
        "lint:docs": "eslint -c .eslintrc.json ./docs/**/*.{ts,tsx} --no-error-on-unmatched-pattern",
        "lint:packages": "eslint -c .eslintrc.json ./packages/**/**/*.{ts,tsx}",
        "test": "concurrently --raw npm:test:*",
        "test:docs": "",
        "test:packages": "",
        "clean": "concurrently --raw npm:clean:*",
        "clean:turbo": "turbo clean && rimraf ./node_modules/.cache/turbo",
        "create:component": "plop component",
        "create:package": "plop package",
        "version": "changeset version",
        "release": "changeset publish"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordstar.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordstar/issues"
    },
    "homepage": "https://nordstar.nordcom.io/",
    "dependencies": {
        "@changesets/cli": "2.26.2",
        "@changesets/types": "5.2.1",
        "@nordcom/prettier": "0.1.1"
    },
    "devDependencies": {
        "@changesets/changelog-github": "0.4.8",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@typescript-eslint/eslint-plugin": "6.9.0",
        "concurrently": "8.2.2",
        "eslint": "8.52.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-unused-imports": "3.0.0",
        "husky": "8.0.3",
        "lint-staged": "15.0.2",
        "plop": "4.0.0",
        "prettier": "3.0.3",
        "prettier-eslint": "16.1.2",
        "rimraf": "5.0.5",
        "tsup": "7.2.0",
        "turbo": "1.10.16",
        "typescript": "5.2.2"
    }
}
