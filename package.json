{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "nordstar",
    "type": "module",
    "version": "0.0.0",
    "description": "An opinionated component library for building human-centric user interfaces.",
    "private": true,
    "main": "index.js",
    "engines": {
        "node": ">=22"
    },
    "workspaces": [
        "packages/core/*",
        "packages/components/*",
        "packages/storybook",
        "docs"
    ],
    "scripts": {
        "dev": "concurrently -i pnpm:dev:* --",
        "dev:docs": "turbo dev --filter=@nordcom/nordstar-docs",
        "dev:storybook": "turbo dev --filter=@nordcom/nordstar-storybook",
        "dev:packages": "turbo dev --filter=!@nordcom/nordstar-docs --filter=!@nordcom/nordstar-storybook --filter=!@nordcom/nordstar --filter=!@nordcom/nordstar-system",
        "build": "turbo build",
        "build:docs": "turbo build --filter=@nordcom/nordstar-docs",
        "build:packages": "turbo build --filter=!@nordcom/nordstar-docs --filter=!@nordcom/nordstar-storybook",
        "build:storybook": "turbo build --filter=@nordcom/nordstar-storybook",
        "typecheck": "turbo typecheck",
        "lint": "eslint --ignore-pattern ./packages \"./**/*.{cjs,mjs,ts,tsx}\"",
        "prettier": "prettier --check  \"./**/*.{cjs,mjs,ts,tsx,scss,json}\" \"!\\packages/**\"",
        "format": "concurrently -i pnpm:format:*",
        "format:docs": "turbo format --filter=@nordcom/nordstar-docs",
        "format:packages": "turbo format --filter=!@nordcom/nordstar-docs --filter=!@nordcom/nordstar-storybook",
        "format:eslint": "eslint --fix --ignore-pattern ./packages \"./**/*.{cjs,mjs,ts,tsx}\"",
        "format:prettier": "prettier --write \"./**/*.{cjs,mjs,ts,tsx,scss,json}\" \"!\\packages/**\"",
        "test": "vitest run --coverage",
        "clean": "turbo clean --filter=!@nordcom/nordstar-docs --filter=!@nordcom/nordstar-storybook && concurrently -i pnpm:clean:*",
        "clean:repo": "rm -rf dist coverage storybook-static .turbo .next .tsbuildinfo *.log",
        "clean:docs": "turbo clean --filter=@nordcom/nordstar-docs",
        "clean:storybook": "turbo clean --filter=@nordcom/nordstar-storybook",
        "create": "plop --plopfile ./plop/plopfile.js",
        "create:component": "plop --plopfile ./plop/plopfile.js component",
        "version": "changeset version",
        "postversion": "pnpm install",
        "version:unstable": "changeset version --snapshot unstable",
        "release": "changeset publish",
        "release:unstable": "changeset publish --snapshot --no-git-tag --tag unstable",
        "changeset": "changeset"
    },
    "author": {
        "name": "Nordcom AB",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam Sandstr√∂m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordstar.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordstar/issues"
    },
    "homepage": "https://nordstar.dev/",
    "devDependencies": {
        "@changesets/changelog-github": "0.5.1",
        "@changesets/cli": "2.29.4",
        "@changesets/types": "6.1.0",
        "@codecov/vite-plugin": "1.9.1",
        "@happy-dom/global-registrator": "17.6.3",
        "@nordcom/prettier": "0.1.1",
        "@storybook/react": "9.0.4",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@types/node": "20.16.11",
        "@types/react-dom": "18.3.0",
        "@types/react": "18.3.10",
        "@typescript-eslint/eslint-plugin": "8.13.0",
        "@typescript-eslint/parser": "8.13.0",
        "@vitejs/plugin-react": "4.3.3",
        "@vitest/coverage-v8": "2.1.4",
        "chalk": "5.3.0",
        "concurrently": "9.1.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jsdoc": "50.4.3",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-unused-imports": "4.1.4",
        "eslint": "8.57.1",
        "glob": "11.0.0",
        "happy-dom": "15.11.0",
        "inquirer-directory": "2.2.0",
        "inquirer": "12.0.1",
        "lint-staged": "15.2.10",
        "magic-string": "0.30.12",
        "plop": "4.0.1",
        "prettier-eslint": "16.3.0",
        "prettier-plugin-tailwindcss": "0.6.8",
        "prettier": "3.6.2",
        "react-dom": "19.1.0",
        "react": "19.1.0",
        "rollup-preserve-directives": "1.1.2",
        "sass": "1.80.6",
        "strip-ansi": "7.1.0",
        "tailwindcss": "3.4.14",
        "turbo": "2.5.4",
        "typescript-plugin-css-modules": "5.1.0",
        "typescript": "5.8.3",
        "vite-plugin-dts": "4.5.4",
        "vite-plugin-lib-inject-css": "2.1.1",
        "vite-tsconfig-paths": "5.1.4",
        "vite": "7.0.0",
        "vitest": "2.1.4"
    },
    "packageManager": "pnpm@9.12.3"
}
